  "ingredients": [
    "2 cups spinach",
    "1/2 cup chopped kale",
    "1/2 cup chopped cucumber",
    "1/2 cup chopped celery",
    "1 banana",
    "1 cup unsweetened almond milk",
    "1/2 cup ice"
